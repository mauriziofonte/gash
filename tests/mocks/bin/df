#!/usr/bin/env bash

# Minimal df mock for disk_usage_fs.
# Prints a small table compatible with: df -hT

cat <<'OUT'
Filesystem     Type  Size Used Avail Use% Mounted on
/dev/sda1      ext4   50G  10G   38G  21% /
tmpfs          tmpfs  16G     0   16G   0% /run/user/1000
/dev/sdb1      xfs   100G  20G   80G  20% /data
OUT
